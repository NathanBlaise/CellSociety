<Simulation>

	<Attributes>
		<Type>RPS</Type>
		<Title>Rock, Paper, Scissors Simulation</Title>
		<Author>Ben Welton</Author>
	</Attributes>

	<Variables>
		<Variable name="lifeLength" value="10" maximum="10"></Variable>
	</Variables>

	<Cells>
		<Cell color="#FDFEFE" proportion="10">
			<Name>EMPTY</Name>
			<Value>0</Value>
		</Cell>

		<Cell color="#E74C3C" proportion="30">
			<Name>ROCK</Name>
			<Value>1</Value>
		</Cell>

		<Cell color="#3498DB" proportion="30">
			<Name>PAPER</Name>
			<Value>2</Value>
		</Cell>
		
		<Cell color="#239B56" proportion="30">
			<Name>SCISSORS</Name>
			<Value>2</Value>
		</Cell>
	</Cells>

	<Grid>
		<Size>10</Size>
		<Type>Normal</Type>
		<Visibile>true</Visibile>
	</Grid>

</Simulation> 