<Simulation>

	<Attributes>
		<Type>Foraging</Type>
		<Title>Foraging Simulation</Title>
		<Author>Ben Welton</Author>
	</Attributes>
	
	<Variables>
		<Variable name="newAntsBorn" value="2" maximum="10"></Variable>
		<Variable name="maxPheromones" value="10" maximum="100"></Variable>
	</Variables>

	<Cells>
		<Cell color="#FDFEFE" proportion="10">
			<Name>EMPTY</Name>
			<Value>0</Value>
		</Cell>

		<Cell color="#F7DC6F" proportion="70">
			<Name>FOOD</Name>
			<Value>1</Value>
		</Cell>

		<Cell color="#2874A6" proportion="20">
			<Name>NEST</Name>
			<Value>2</Value>
		</Cell>
	</Cells>

	<Grid>
		<Size>10</Size>
		<Type>Normal</Type>
		<Visibile>true</Visibile>
	</Grid>

</Simulation> 